{
  "name": "wxyc_backend",
  "version": "1.0.0",
  "description": "An API service for the flowsheet database of WXYC",
  "main": "./dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "dev": "nodemon -w src -e ts --exec \"npm run start\"",
    "lint": "eslint . --ext .ts",
    "test": "echo \"no tests specified\"",
    "generate": "drizzle-kit generate:pg --schema ./src/db/schema.ts --out ./src/db/migrations",
    "migrate": "node src/db/migrations/migration_script.mjs",
    "schema_drop": "drizzle-kit drop --out ./src/db/migrations"
  },
  "author": "AyBruno",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.4",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "drizzle-kit": "^0.19.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.0",
    "express": "^4.18.2",
    "postgres": "^3.3.5"
  }
}
